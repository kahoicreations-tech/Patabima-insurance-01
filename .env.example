// Environment configuration for AWS integration
// Copy this to .env file and update with your AWS values

# AWS Configuration
AWS_REGION=us-east-1

# Cognito User Pool Configuration
AWS_USER_POOL_ID=us-east-1_xxxxxxxxx
AWS_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
AWS_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# OAuth Configuration
AWS_OAUTH_DOMAIN=your-domain.auth.us-east-1.amazoncognito.com
AWS_REDIRECT_SIGN_IN=exp://localhost:8083/
AWS_REDIRECT_SIGN_OUT=exp://localhost:8083/

# API Gateway
AWS_API_ENDPOINT=https://xxxxxxxxxx.execute-api.us-east-1.amazonaws.com/prod

# S3 Storage
AWS_S3_BUCKET=patabima-storage-bucket

# Pinpoint Analytics
AWS_PINPOINT_APP_ID=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Development vs Production
NODE_ENV=development

# App Configuration
APP_NAME=PataBima
APP_VERSION=1.0.0
API_TIMEOUT=30000

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_OFFLINE_MODE=true
ENABLE_FILE_UPLOAD=true

# Debug Settings
DEBUG_AWS=false
DEBUG_API=false
LOG_LEVEL=info
