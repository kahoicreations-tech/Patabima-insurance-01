{
  "Version": "2012-10-17",
  "Statement": [
    { "Effect": "Allow", "Action": [ "logs:CreateLogGroup", "logs:CreateLogStream", "logs:PutLogEvents" ], "Resource": "arn:aws:logs:*:*:*" },
    { "Effect": "Allow", "Action": [ "textract:AnalyzeDocument", "textract:DetectDocumentText" ], "Resource": "*" },
    { "Effect": "Allow", "Action": [ "s3:GetObject", "s3:PutObject" ], "Resource": "arn:aws:s3:::patabima-backend-dev-uploads/*" },
    { "Effect": "Allow", "Action": [ "sqs:ReceiveMessage", "sqs:DeleteMessage", "sqs:GetQueueAttributes" ], "Resource": "arn:aws:sqs:us-east-1:313530061018:patabima-textract-dev" }
  ]
}
