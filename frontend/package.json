  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend:local": "powershell -NoProfile -ExecutionPolicy Bypass -File ./switch-to-local.ps1",
    "backend:ec2": "powershell -NoProfile -ExecutionPolicy Bypass -File ./switch-to-ec2.ps1",
    "underwriters": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/print-underwriters.ps1",
    "build:android": "eas build --platform android --profile preview",
    "build:android:production": "eas build --platform android --profile production-apk",
    "build:android:aab": "eas build --platform android --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "build:all": "eas build --platform all --profile production",
    "generate:icons": "node scripts/generate-icons.js",
    "prebuild": "expo prebuild --clean",
    "postinstall": "expo install --fix"
  },
